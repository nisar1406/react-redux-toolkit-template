{
  "name": "react-redux-toolkit-template",
  "version": "1.0.0",
  "type": "module",
  "license": "MIT",
    "description": "Modern template to start creating react app using webpack and typescript.",
    "keywords": [
        "react",
        "typescript",
        "vite",
        "redux-toolkit",
        "react-router",
        "tailwindcss",
        "daisyui",
        "eslint",
        "prettier",
        "husky",
        "lint-staged"
    ],
    "author": "Nisar Shaikh (https://github.com/nisar1406)",
    "homepage": "https://github.com/nisar1406/react-redux-toolkit-template#readme",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/nisar1406/react-redux-toolkit-template.git"
    },
    "bugs": {
        "url": "https://github.com/nisar1406/react-redux-toolkit-template/issues"
    },
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint src --ext .js,.ts,.tsx --ignore-pattern '!.*.js'",
    "lint:fix": "eslint src --ext .js,.ts,.tsx --ignore-pattern '!.*.js' --fix",
    "preview": "vite preview",
    "prepare": "husky install"
  },
  "lint-staged": {
    "*.{ts,tsx}": "eslint src --ext .js,.ts,.tsx --ignore-pattern '!.*.js' --fix"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^2.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^9.1.0",
    "react-router-dom": "^6.21.2"
  },
  "devDependencies": {
    "@heroicons/react": "^2.1.1",
    "@hookform/resolvers": "^3.3.4",
    "@tailwindcss/typography": "^0.5.10",
    "@trivago/prettier-plugin-sort-imports": "^4.3.0",
    "@types/node": "^20.11.5",
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@types/react-redux": "^7.1.33",
    "@types/react-router-dom": "^5.3.3",
    "@typescript-eslint/eslint-plugin": "^7.4.0",
