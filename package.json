    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "husky": ">=7",
    "lint-staged": ">=10",
    "moment": "^2.30.1",
    "prettier": "^3.2.5",
    "react-hook-form": "^7.51.2",
    "react-toastify": "^10.0.3",
    "tailwindcss": "^3.0.0",
    "theme-change": "^2.5.0",
